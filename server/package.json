{
  "name": "server",
  "version": "1.0.0",
  "description": "server for pizza delivery app",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "build-ts": "rm -rf dist/ && tsc",
    "start": "npm run serve",
    "serve": "NODE_ENV=production node dist/index.js",
    "watch-node": "nodemon",
    "watch-ts": "tsc -w",
    "migration:generate": "ts-node ./node_modules/.bin/typeorm migration:generate",
    "migration:run": "ts-node ./node_modules/.bin/typeorm migration:run",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "typescript",
    "node",
    "express",
    "bookshelf",
    "typescript",
    "boilerplate",
    "tslint"
  ],
  "author": "SofiaTernovskaya",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.1",
    "@types/joi": "^14.3.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "nodemon": "^1.18.10",
    "prettier": "1.19.1",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/morgan": "^1.9.1",
    "bcrypt": "^3.0.5",
    "body-parser": "^1.20.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.18.2",
    "express-joi-validator": "^2.0.1",
    "express-jwt": "^5.3.1",
    "http-status-codes": "^1.3.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "mysql": "^2.16.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.15"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,json,md}": [
      "eslint --fix"
    ]
  }
}
